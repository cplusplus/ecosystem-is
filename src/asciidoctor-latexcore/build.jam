require-b2 5.2 ;

project /grafikrobot/asciidoctor-latexcore ;

path-constant here : . ;

alias latexcore
	: usage-requirements
		<asciidoctor-latexcore.flags>"-r $(here)/latexcore.rb"
		<asciidoctor-latexcore.flags>"-b latexcore"
		<dependency>latexcore.rb
	;
explicit latexcore ;
